{
  "name": "@acontplus/ng-notifications",
  "version": "2.0.2",
  "description": "Comprehensive Angular notification system with toast notifications (ngx-toastr), alerts (SweetAlert2), snackbars, theme detection, notification providers, and configurable styling. Supports multiple notification types with Angular Material integration.",
  "peerDependencies": {
    "@acontplus/ui-kit": "^1.0.2",
    "@angular/common": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/material": "^21.0.0",
    "ngx-toastr": "^19.1.0",
    "sweetalert2": "^11.26.2",
    "rxjs": "^7.8.2"
  },
  "sideEffects": true,
  "main": "fesm2022/acontplus-ng-notifications.mjs",
  "module": "fesm2022/acontplus-ng-notifications.mjs",
  "typings": "index.d.ts",
  "files": [
    "index.d.ts",
    "*.d.ts",
    "**/*.d.ts",
    "fesm2022/**/*",
    "src/**/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "acontplus",
    "angular",
    "angular-material",
    "material-design",
    "notifications",
    "ngx-toastr",
    "sweetalert2",
    "toast-notifications",
    "alert-dialogs",
    "snackbar",
    "notification-system",
    "notification-providers",
    "theme-detection",
    "dark-mode-detection",
    "configurable-styling",
    "notification-types",
    "material-integration",
    "scss-theming",
    "typescript",
    "enterprise",
    "frontend"
  ],
  "author": "Ivan Paz <ifer343@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Acontplus-S-A-S/acontplus-libs/issues"
  },
  "homepage": "https://github.com/Acontplus-S-A-S/acontplus-libs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Acontplus-S-A-S/acontplus-libs.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
