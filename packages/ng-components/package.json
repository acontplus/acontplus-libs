{
  "name": "@acontplus/ng-components",
  "version": "2.0.1",
  "description": "Comprehensive Angular Material UI component library featuring dynamic Tabulator tables, theme toggle with dark mode, dialog wrappers, autocomplete components, cards, buttons, icons, input chips, spinners, directives, pipes, and SCSS styling utilities.",
  "peerDependencies": {
    "@acontplus/ui-kit": "^1.0.2",
    "@angular/cdk": "^21.0.5",
    "@angular/common": "^21.0.6",
    "@angular/core": "^21.0.6",
    "@angular/material": "^21.0.5",
    "tabulator-tables": "^6.3.1"
  },
  "sideEffects": true,
  "main": "fesm2022/acontplus-ng-components.mjs",
  "module": "fesm2022/acontplus-ng-components.mjs",
  "typings": "index.d.ts",
  "files": [
    "index.d.ts",
    "*.d.ts",
    "**/*.d.ts",
    "fesm2022/**/*",
    "src/**/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "acontplus",
    "angular",
    "angular-material",
    "material-design",
    "ui-components",
    "tabulator-tables",
    "dynamic-tables",
    "data-tables",
    "cards",
    "dialog-wrapper",
    "theme-toggle",
    "dark-mode",
    "theme-switcher",
    "overlay-service",
    "autocomplete-wrapper",
    "spinner",
    "buttons",
    "icons",
    "input-chip",
    "to-uppercase-directive",
    "status-display-pipe",
    "get-total-pipe",
    "pagination",
    "scss-mixins",
    "scss-variables",
    "custom-dialog",
    "mat-table",
    "typescript",
    "enterprise",
    "frontend"
  ],
  "author": "Ivan Paz <ifer343@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Acontplus-S-A-S/acontplus-libs/issues"
  },
  "homepage": "https://github.com/Acontplus-S-A-S/acontplus-libs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Acontplus-S-A-S/acontplus-libs.git"
