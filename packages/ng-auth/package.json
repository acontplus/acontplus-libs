{
  "name": "@acontplus/ng-auth",
  "version": "1.2.2",
  "description": "Comprehensive Angular authentication module with JWT token management, route guards, CSRF protection, URL redirection, session handling, and clean architecture patterns. Includes login components, auth interceptors, and DDD-based repositories.",
  "peerDependencies": {
    "@acontplus/core": "^1.1.1",
    "@acontplus/ng-infrastructure": "^1.1.2",
    "@acontplus/ng-config": "^1.1.1",
    "@angular/common": "^20.3.9",
    "@angular/core": "^20.3.9",
    "@angular/router": "^20.3.9",
    "@angular/forms": "^20.3.9",
    "@angular/platform-browser": "^20.3.9",
    "@angular/material": "^20.2.12",
    "jwt-decode": "^4.0.0",
    "rxjs": "^7.8.1"
  },
  "sideEffects": false,
  "main": "fesm2022/acontplus-ng-auth.mjs",
  "module": "fesm2022/acontplus-ng-auth.mjs",
  "typings": "index.d.ts",
  "files": [
    "index.d.ts",
    "*.d.ts",
    "**/*.d.ts",
    "fesm2022/**/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "acontplus",
    "angular",
    "authentication",
    "jwt",
    "jwt-decode",
    "auth-guard",
    "route-guards",
    "auth-interceptor",
    "csrf-interceptor",
    "csrf-protection",
    "url-redirect",
    "session-management",
    "token-management",
    "login-component",
    "auth-providers",
    "auth-repository",
    "auth-stores",
    "ddd",
    "clean-architecture",
    "hexagonal-architecture",
    "use-cases",
    "domain-models",
    "typescript",
    "angular-material",
    "enterprise",
    "frontend"
  ],
  "author": "Ivan Paz <ifer343@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Acontplus-S-A-S/acontplus-libs/issues"
